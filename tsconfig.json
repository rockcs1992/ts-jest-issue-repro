{
    "compileOnSave": true,
    "compilerOptions": {
        "allowJs": true,
        "baseUrl": ".",
        "composite": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "importHelpers": true,
        "forceConsistentCasingInFileNames": true,
        "noEmitHelpers": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "esModuleInterop": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "moduleResolution": "node",
        "removeComments": false,
        "sourceMap": true,
        "jsx": "react",
        "inlineSources": true,
        "module": "commonjs",
        "target": "ES2019",
        "types": ["jest", "node"],
    },
    "include": [
      "**/*"
    ],
    "exclude": [
    ]
  }
  